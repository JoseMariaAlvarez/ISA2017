<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juanignacio.controlinr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="es.uma.controlinr.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="es.uma.controlinr.MenuGuias"
            android:parentActivityName="es.uma.controlinr.MainActivity"
            >
        </activity>
        <activity android:name="es.uma.controlinr.GuiaPaciente"
            android:parentActivityName="es.uma.controlinr.MenuGuias"
            >
        </activity>
        <activity android:name="es.uma.controlinr.GuiaProfesional"
            android:parentActivityName="es.uma.controlinr.MenuGuias"
            >
        </activity>
        <activity android:name="es.uma.controlinr.GuiaEmergencia"
            android:parentActivityName="es.uma.controlinr.MenuGuias"
            >
        </activity>
    </application>

</manifest>